{
  "name": "react-vite-boilerplate",
  "version": "1.0.0",
  "description": "ReactJS & Vite boilerplate",
  "main": "main.tsx",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "vite preview",
    "tsc": "tsc",
    "build": "tsc && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grimowsky/react-vite-boilerplate.git"
  },
  "keywords": [
    "react",
    "vite",
    "boilerplate"
  ],
  "author": "Grimm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grimowsky/react-vite-boilerplate/issues"
  },
  "homepage": "https://github.com/Grimowsky/react-vite-boilerplate#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-istanbul": "^0.32.0",
    "@vitest/ui": "^0.31.4",
    "eslint-config-grimowsky": "^1.0.8",
    "history": "^5.3.0",
    "jsdom": "^22.1.0",
    "typescript": "5.0.3",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.32.0"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-grimowsky"
    ],
    "root": true
  }
}
